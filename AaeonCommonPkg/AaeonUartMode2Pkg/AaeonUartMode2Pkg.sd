//------------------------------------------------------------
// Define Macro
//------------------------------------------------------------

#define AAEON_UARTMODE_ITEMS( \
	UART_NUM, \
	CHECKBOX_AAEON_UARTMODE_DEFAULT, \
	NUMERIC_AAEON_UARTMODE_UID_DEFAULT, \
	ONEOF_AAEON_UARTMODE_SOURCE_DEFAULT, \
	ONEOF_AAEON_UARTMODE_UART_SOURCE_DEFAULT, \
	DEFAULT_AAEON_UARTMODE, \
	NUMERIC_AAEON_UARTMODE_GPIO0_DEFAULT, \
	NUMERIC_AAEON_UARTMODE_GPIO1_DEFAULT, \
	NUMERIC_AAEON_UARTMODE_GPIO2_DEFAULT) \
	CHECKBOX_AAEON_UARTMODE(UART_NUM, CHECKBOX_AAEON_UARTMODE_DEFAULT) \
	suppressif ideqval SETUP_DATA.AaeonUartMode2[UART_NUM] == 0; \
		ONEOF_AAEON_UARTMODE_DEFAULT(UART_NUM, DEFAULT_AAEON_UARTMODE) \
		NUMERIC_AAEON_UARTMODE_UID(UART_NUM, NUMERIC_AAEON_UARTMODE_UID_DEFAULT) \
		ONEOF_AAEON_UARTMODE_SOURCE(UART_NUM, ONEOF_AAEON_UARTMODE_SOURCE_DEFAULT) \
		ONEOF_AAEON_UARTMODE_UART_SOURCE(UART_NUM, ONEOF_AAEON_UARTMODE_UART_SOURCE_DEFAULT) \
		NUMERIC_AAEON_UARTMODE_GPIO0(UART_NUM, NUMERIC_AAEON_UARTMODE_GPIO0_DEFAULT) \
		NUMERIC_AAEON_UARTMODE_GPIO1(UART_NUM, NUMERIC_AAEON_UARTMODE_GPIO1_DEFAULT) \
		suppressif ideqval SETUP_DATA.AaeonUartMode2Transceiver == 0; \
			NUMERIC_AAEON_UARTMODE_GPIO2(UART_NUM, NUMERIC_AAEON_UARTMODE_GPIO2_DEFAULT) \
		endif; \
	endif;
