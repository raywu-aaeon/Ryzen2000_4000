	Name  = "SIO_SETUP_USE_UID_AS_NUMBERS"
	Value  = "1"
	Help  = "If multiple number of same type devices shown in Setup\this will tell to use LDs UID as Device Number in Setup\If set to OFF 1 base ordering numbers will be used."
	TokenType = Boolean
	TargetH = Yes
End

